            "name": "Ivan Karatayev",
            "email": "ivan.karatayev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/log": "^1.1",
        "illuminate/support": "^7",
        "monolog/monolog": "^2.1",
        "fluent/logger": "^1.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLoggerExtension\\": "src/LaravelLoggerExtension"
